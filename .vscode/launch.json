{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "editor",
      "runtimeExecutable": "${workspaceFolder}/editor/node_modules/.bin/electron",
      "program": "${workspaceFolder}/editor/main.js",
      "cwd": "${workspaceFolder}/editor/",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "timer Program",
      "cwd": "${workspaceFolder}/timer",
      "program": "${workspaceFolder}/timer/timerserver.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\server\\app.js"
    }
  ]
}